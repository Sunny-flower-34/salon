<div class="section">
  <h1>Tags list</h1>
  <p>（）内の数字はそのタグをつけているユーザーの人数です</p>
  <% @tags.each do |tag| %>
  <%= link_to users_path(tag) ,class: "tag" do %>
  <span style="background-color: #e9e9e9; border-radius: 5px;"><%= tag %></span>
  <span>(<%= tag.taggings_count %>)</span>
  <% end %>
  <% end %>
</div>